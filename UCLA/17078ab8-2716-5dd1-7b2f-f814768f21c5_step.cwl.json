{
  "cwlVersion": "v1.1",
  "class": "CommandLineTool",
  "requirements": {
    "InlineJavascriptRequirement": {},
    "DockerRequirement": {
      "dockerImageId": "robert666666/validation"
    },
    "ShellCommandRequirement": {},
    "InitialWorkDirRequirement": {
      "listing": [
        {
          "entry": "$(inputs.parameters)",
          "writable": true
        },
        {
          "entry": "$(inputs.X_train)",
          "writable": true
        },
        {
          "entry": "$(inputs.model)",
          "writable": true
        },
        {
          "entry": "$(inputs.y_train)",
          "writable": true
        },
        {
          "entry": "$(inputs.X_test)",
          "writable": true
        },
        {
          "entry": "$(inputs.y_test)",
          "writable": true
        }
      ]
    }
  },
  "inputs": {
    "parameters": {
      "type": "File",
      "inputBinding": {
        "position": 6
      }
    },
    "X_train": {
      "type": "File",
      "inputBinding": {
        "position": 1
      }
    },
    "model": {
      "type": "File",
      "inputBinding": {
        "position": 5
      }
    },
    "y_train": {
      "type": "File",
      "inputBinding": {
        "position": 2
      }
    },
    "X_test": {
      "type": "File",
      "inputBinding": {
        "position": 3
      }
    },
    "y_test": {
      "type": "File",
      "inputBinding": {
        "position": 4
      }
    }
  },
  "outputs": {
    "shapley_result": {
      "type": "File",
      "outputBinding": {
        "glob": "shaply_summary.png"
      }
    },
    "bootstrapping_result": {
      "type": "File",
      "outputBinding": {
        "glob": "bootstrapping.png"
      }
    },
    "ROC_result": {
      "type": "File",
      "outputBinding": {
        "glob": "ROC.png"
      }
    },
    "LIME_interpretation": {
      "type": "File",
      "outputBinding": {
        "glob": "LIME_interpretation.png"
      }
    }
  },
  "arguments": [
    {
      "shellQuote": false,
      "valueFrom": "ln -s /code/* ./\npython verification_script.py"
    }
  ]
}
